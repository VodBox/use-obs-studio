{
  "name": "use-obs-studio",
  "version": "1.0.0",
  "description": "React Hooks for working with OBS Studio's obs-browser plugin",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "files": [
    "lib/",
    "esm/"
  ],
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:types": "tsc --noEmit",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "npm run build:cjs && npm run build:es",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VodBox/use-obs-studio"
  },
  "keywords": [
    "custom-hooks",
    "obs-studio",
    "react",
    "react-hooks",
    "typescript"
  ],
  "author": "VodBox (Dillon Pentz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VodBox/use-obs-studio/issues"
  },
  "homepage": "https://github.com/VodBox/use-obs-studio#readme",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "@types/obs-studio": "^2.16.1",
    "@types/react": "17.0.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "react": "^17.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {}
}